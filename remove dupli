package hero;
public class removeduplicaters{
	public static void main(String[] args)
	{
		int[] arr= {11,11,3,4,5,6,7};
		int[] b=new int[10];
		int i,j,k=1;
		int l=arr.length;
		for(i=0;i<l;i++)
		{
			for(j=0;j<k;j++)
			{
				if(arr[i]==b[j])
				{
					break;
				}
			}
			if(j==k)
			{
				b[k]=arr[i];
				k++;
			}
		}
		for(i=0;i<k;i++)
		{
			System.out.println(b[i]);
		}
	}
}
